<fieldset>
  <legend>Coach Information</legend>
  <div class="small-4 large-4 columns">
    <%= f.text_field :coach_first, placeholder: "Coach's First Name" %>
  </div>
  <div class="small-4 large-4 columns">
    <%= f.text_field :coach_last, placeholder: "Coach's Last Name" %>
  </div>
  <div class="small-4 large-4 columns">
    <%= f.email_field :coach_email, placeholder: "Coach's Email" %>
  </div>
</fieldset>