<div class="row">
  <h2>Sign up for SWSDI</h2>

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { multipart: true }) do |f| %>
    <%= devise_error_messages! %>
    <%= render 'userinfo', :f => f  %>
    <%= render 'personalinfo', :f => f  %>
    <%= render 'parentinfo', :f => f  %>
    <%= render 'coachinfo', :f => f  %>
    <div><%= f.submit "Sign up", class: "button primary" %></div>
  <% end %>

  <%= render "devise/shared/links" %>
</div>