<div class="row">

  <h1>Current Admins</h1>

  <ul>
  <% @admins.each do |admin| %>
    <li class="panel"><%= admin.admin_profile.full_name %> | <%= admin.email %> | <% if current_admin.role == "super" %><%= link_to "Delete Admin User", admin_delete_path, method: 'delete'  %> <% end %></li>
  <% end %>
  </ul>

</div>